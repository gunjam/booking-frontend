{
  "name": "booking-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">6.0.0"
  },
  "scripts": {
    "test": "xo"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "date-fns": "^1.28.4",
    "express": "^4.15.2",
    "got": "^6.7.1",
    "govuk-elements-marko": "^4.0.0",
    "govuk-elements-sass": "^3.0.2",
    "govuk-template-marko": "^1.1.0",
    "helmet": "^3.6.0",
    "i18next": "^8.1.0",
    "i18next-express-middleware": "^1.0.5",
    "i18next-node-fs-backend": "^1.0.0",
    "lasso": "~2.11.10",
    "lasso-sass": "^2.0.0",
    "marko": "^4.2.8",
    "marko-i18next": "^0.2.0",
    "nodemailer": "^4.0.1",
    "shrink-ray": "^0.1.3",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "env": [
      "node",
      "browser",
      "jquery"
    ],
    "ignores": [
      "./**/*.marko.js"
    ],
    "overrides": [
      {
        "files": "src/common/application.js",
        "esnext": false
      }
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
